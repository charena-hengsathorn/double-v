{
  "name": "double-v-api-server",
  "version": "1.0.0",
  "description": "Unified API server for Double V dashboard - proxies to Strapi and Predictive Service",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "check-env": "node scripts/check-env.js"
